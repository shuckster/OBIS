{
  "name": "OBIS",
  "description": "A JavaScript Framework for downloading Bank Statements",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["http://localhost:4000/chrome.html"],
      "css": ["ui.css"],
      "js": ["obis-hsbc-uk.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["statement.css"],
      "matches": ["https://obis.styles/*"]
    }
  ]
}
