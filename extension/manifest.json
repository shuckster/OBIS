{
  "name": "OBIS",
  "description": "A JavaScript Framework for downloading Bank Statements",
  "version": "2.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": [
        "http://localhost:4000/",
        "https://services.online-banking.hsbc.co.uk/",
        "https://www.services.online-banking.hsbc.co.uk/",
        "https://www.hsbc.co.uk/"
      ],
      "css": [
        "ui.css"
      ],
      "js": [
        "obis-hsbc-uk.js"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "statement.css"
      ],
      "matches": [
        "https://obis.styles/*"
      ]
    }
  ]
}